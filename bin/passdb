#!/usr/bin/env ruby

source_root = File.expand_path(File.dirname(__FILE__) + "/..")
$LOAD_PATH.unshift("#{source_root}/lib")

require 'passdb/cli'

begin
  Passdb::CLI.start
rescue Exception => e
  puts e.message
  puts e.backtrace.join("\n")
  exit e.status_code
#rescue Interrupt => e
#  puts "\nQuitting..."
#  exit 1
end

